<!--
  This file is used by the make-project script to figure out the latest version of
  JCatapult Core.
-->
<project xmlns="http://www.inversoft.org/schemas/savant-2.0/project"
         name="make-project"
         group="jcatapult.org"
         version="1.0">

  <dependencies id="jcatapult-core">
    <artifactGroup type="compile">
      <artifact group="jcatapult.org" project="jcatapult-core" name="jcatapult-core" version="{latest}" type="jar"/>
    </artifactGroup>
  </dependencies>

  <workflow id="workflow.project">
    <fetchWorkflow>
      <process type="unifiedLocalCache"/>
      <process type="unified" url="http://savant.inversoft.org/"/>
    </fetchWorkflow>
    <publishWorkflow>
      <process type="unifiedLocalCache"/>
    </publishWorkflow>
  </workflow>
</project>